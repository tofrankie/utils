{
  "entryPoints": ["src/index.ts"],
  "out": "docs/api",
  "theme": "default",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "README.md",
  "name": "JavaScript Utils API",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "sort": ["source-order"],
  "categorizeByGroup": true,
  "defaultCategory": "Other",
  "categoryOrder": ["Function", "Array", "Object", "String", "Validation", "Other"],
  "navigation": {
    "includeCategories": true,
    "includeGroups": true
  },
  "searchInComments": true,
  "cleanOutputDir": true
}
